{
  "domain": "netatmo",
  "name": "Netatmo Home+Control",
  "after_dependencies": ["cloud", "media_source"],
  "codeowners": ["@fabianocattaneo"],
  "config_flow": true,
  "dependencies": ["application_credentials", "webhook"],
  "documentation": "https://www.home-assistant.io/integrations/netatmo",
  "homekit": {
    "models": ["Healthy Home Coach", "Netatmo Relay", "Presence", "Welcome"]
  },
  "integration_type": "hub",
  "iot_class": "cloud_polling",
  "loggers": ["pyatmo"],
  "requirements": ["pyatmo @ git+https://github.com/fabianocattaneo/pyatmo.git@development"],
  "version": "0.0.1"
}
